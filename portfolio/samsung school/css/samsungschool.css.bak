@charset "utf-8";
@import url(reset.css);

#content { width:1310px; margin:0 auto; padding-top:20px;}
.visual { position:relative; }
.visual h2 { position:absolute; right:20px; bottom:68px; font-size:36px; color:#fff;}
.visual p { position:absolute; right:20px; bottom:18px; text-align:right; color:#fff; font-size:20px;}
.works { padding-top:60px; }
h3 { font-size:34px; font-family:'SamsungIFBd'; padding-bottom:5px; color:#333;}
.works_inner:after { content:""; display:block; clear:both;}
.works_inner li { float:left; margin-left:10px; position:relative; }
.works_inner li:first-child { margin-left:0; }
.works_inner .work_text { position:absolute; top:170px; font-size:24px; color:#fff; width:100%; text-align:center; }
.works_inner .play { font-size:0; width:90px; height:90px; position:absolute; top:72px; left:50%; margin-left:-45px; }

.developed { padding-top:75px;}
.developed h3 { font-size:34px; font-family:'SamsungITBd'; padding-bottom:5px;}
.developed_inner:after { content:""; display:block; clear:both;}
.developed_inner li { float:left; width:430px; margin-left:10px; }
.developed_inner li:first-child { margin-left:0; }
.developed_inner strong { font-size:24px; color:#333; display:inline-block; margin-top:10px; border-bottom:2px solid #35c9ea; padding-bottom:5px;}
.developed_inner p { font-size:18px; color:#666; padding-top:18px;}
.benefits_inner .choice { position:relative; background-color:#486b96; color:#fff;}
.benefits_inner .choice:after {	content: ""; position: absolute; bottom: -15px;	left: 48%; width: 0; height: 0; border-left: 18px solid transparent;	border-right: 18px solid transparent;
	border-top: 18px solid #486b96; margin-left:-18px; }
.benefits_inner .center { margin:0 1px; }
.benefits { padding-top:75px; padding-bottom:60px;}
.benefits h3 { font-size:34px; font-family:'SamsungITBd'; padding-bottom:5px; }
.benefits_inner:after { content:""; display:block; clear:both;}
.benefits_inner li button{ float:left; width:436px; text-align:center; background-color:#eaf0f4; height:70px; line-height:70px;}
.benefits_inner li button { color:#486b96;  font-size:22px; font-family:'SamsungITBd'; font-weight:700;}
.benefits .item1_img { text-align:center; padding-top:80px;  }
.benefits .item1_img:after { content:""; display:block; clear:both;}
.benefits .item1_img li {width:30%; display:inline-block;}
.benefits .item1_text { width:483px; margin-left:30px; padding-top:20px; float:left; box-sizing:border-box;}
.benefits .inner_text:after { content:""; display:block; clear:both;}
.benefits .inner_text li:after { content:""; display:block; clear:both; }
.benefits .inner_text li { padding-bottom:20px; }
.benefits .inner_text li:first-child { padding-top:10px;}
.benefits .item1_text h4{  font-size:28px; color:#3f546f; border-bottom:2px solid #35c8ea; padding-bottom:5px;}
.benefits .item1_text strong { font-size:18px; color:#3f546f; width:77px; float:left; font-family:'SamsungITBd'; padding-left:8px; background:url("../images/left_icon.png") no-repeat 0 10px; box-sizing:border-box;}
.benefits .item1_text p { font-size:18px; color:#3f546f; width:402px; float:left; margin-top:3px; box-sizing:border-box; padding-left:10px;}
.benefits .item_wrap { position:relative; }
.benefits .item_wrap:after { content:""; display:block; clear:both;}
.benefits .text2 h4 { text-align:right;}
.benefits .text2 { /*position:absolute; right:29px; top:0;*/ margin-left:20%; padding-left:0;margin-right:30px;}
.benefits .text2 strong { width:120px;}
.benefits .text2 p { width:362px;}
.features { position:relative; box-sizing:border-box; padding-left:32px; background-color:#f8f8f8;}
.features:after { content:""; display:block; clear:both;}
.features h3 { padding-top:35px; color:#333; padding-bottom:0;}
.features .top_text { font-size:22px; width:61%;}
.features .more { width:300px; height:58px; border:2px solid #486b96; border-radius:10px; font-size:20px; color:#486b96; text-align:left; background:url("../images/seeMore.png") no-repeat 240px 11px; position:absolute; right:29px; top:40px; background-color:#fff;}
.features .more span { padding-left:30px; display:inline-block;}
.features_inner { padding-top:80px;}
.features_inner:after { content:""; display:block; clear:both;}
.features_inner li { float:left; width:50%; text-align:center; padding-bottom:95px;}
.features_inner li strong { display:block; text-align:left; font-size:24px; color:#333; padding-top:32px; border-bottom:2px solid #35c8ea; margin-right:30px; padding-bottom:5px; font-weight:100;}
.features_inner .inner_text{ text-align:left; font-size:18px; color:#666; height:63px; margin-right:30px; padding-top:15px;}

.what { padding-top:80px; padding-bottom:90px;}
.what:after { content:""; display:block; clear:both; }
.what .what_item1, .what .what_item2 { width:50%; float:left;}
.what .what_item1 { padding-top:125px;}
.what .what_item1 h3 { padding-bottom:15px; color:#333333;}
.what .what_item1 h3 span { display:block; }
.what .what_item1 p { width:80%; padding-bottom:82px;}
.what .what_btn { margin-bottom:10px; display:block; width:300px; height:58px; border:2px solid #a3b5ca; border-radius:10px; font-size:20px; color:#486b96; text-align:left; background:url("../images/seeMore.png") no-repeat 240px 11px; background-color:#fff;}
.what .what_btn span { padding-left:30px; display:inline-block;}
.what .what_item1 p { font-size:18px;}
.scroll { position:relative; }
.scroll_inner .top_title{ position:absolute; top:55px; color:#fff; font-size:40px; text-align:center; width:100%; font-style: italic;}
.scroll_inner .top_title:after {content:""; border-bottom:3px solid #fff; width:58px;height:3px; display:block; margin:15px auto;}
.scroll_inner .teacher { position:absolute; top:260px; width:100%; text-align:center; font-size:26px; color:#fff;}
.scroll_inner .teacher span { display:block; font-style: italic; font-family:'SamsungIFBd'; margin-top:3px;}
.scroll_inner .teacher:after { content:""; background:url("../images/scroll_bot.png") no-repeat center; display:block; height:30px; margin-top:40px; }
.scroll_inner .us {position: absolute; bottom:25px; width:100%; text-align:center;}
.scroll_inner .us strong{ font-size:36px; font-family:'SamsungITBd'; color:#fff;}
.scroll_inner .us p { font-size:20px; color:#fff; padding-bottom:30px; }
.scroll_inner .us .email {font-size:20px; width:280px; height:48px; border:3px solid #00deff; border-radius:10px; color:#00deff;}
.scroll_inner .scroll_btn { position:absolute; right:15px; top:193px; width:26px; height:102px; background-color:#3f546f; text-align:center; border-radius:10px; }
.scroll_btn button { font-size:0; width:14px; height:14px; border:3px solid #fff; border-radius:7px;}
.scroll_btn .on { background-color:#fff; }
.scroll { background:url("../images/contact.jpg") no-repeat;}

.product { padding-top:75px;}
.product h3 { color:#666; padding-bottom:10px; font-family:'SamsungIFRg'; font-weight:100;}
.product_inner:after { content:""; display:block; clear:both;}
.product_inner li{ float:left; border:1px solid #e8e8e8; margin-right:10px; }
.product_inner li p { padding-left:20px; padding-top:18px; background-color:#f1f1f1; padding-bottom:64px; font-weight:700; font-size:20px;}
.product_inner li a { color:#000; }
.insight { padding-top:35px; padding-bottom:40px; }
.insight h3 { color:#666; font-family:'SamsungIFRg'; font-weight:100;}
.insight_box:after { content:""; display:block; clear:both;}
.insight_box li {position:relative; padding-top:17px; width:320px; float:left; background-color:#f1f1f1; margin-right:10px; box-sizing:border-box; padding-left:20px; height:260px;}
.insight .scale { font-size:25px;  }
.insight .textarea { font-size:16px; padding-top:20px; }
.insight .pdf { color:#1e68b6; font-weight:700; position:absolute; bottom:14px; padding-right:20px; background:url("../images/pdf.png") no-repeat right;}

.m_benefits { display:none;}
.m_visual { display:none; }

@media all and ( max-width:1310px ){
	
	#content { width:100%; }
	.visual img { width:100%; }
	.works_inner li { width:33%; margin-left:0; }
	.works_inner li img { width:100%;} 
	.works_inner .second { margin:0 0.5%;}
	.works_inner .play { top:25%; width:30%; height:40%; margin-left:-15%;}
	.works_inner .work_text { top:70%;}
	.works_inner .play img{ width:80%; height:auto;}
	
	.developed li{ width:33%; margin-left:0; }
	.developed li img { width:100%;}
	.developed .second { margin:0 0.5%; }
	.developed_inner p { width:90%; }
	
	.benefits { width:100%; } 
	.benefits .benefits_inner li{ width:33%; margin:0; float:left;}
	.benefits .benefits_inner button { width:100%; }
	.benefits .second { margin:0 1px;}
	.benefits .item1_img { }
	.item1_img li img{ width:80%; } 
	
	.benefits .text2 { width:40%; padding-left:20px; margin-left:0; float:right;}

	.benefits .item1_text { width:40%;}
	.benefits .item1_text strong { width:15%; }
	.benefits .item1_text p { width:85%; padding-left:50px;}
	.benefits .text2  p { padding-left:80px; }

	.features_inner li img { width:60%; }
	.features .top_text { width:50%;}
	.what .what_item1 { box-sizing:border-box; padding-left:32px; padding-top:7%;}
	.what .what_item2 img{ width:90%; }

	.scroll_inner .top_title {  top:15px;}
	.scroll_inner .scroll_btn { top:210px;}
	.contact { width:100%; height:696px;}
	.product_inner li { width:25%;}
	.product_inner li img { width:100%; }
	 
	.insight_box li { width:25%; }
	.insight_box li img { width:100%; }
	.insight .scale { height:60px; overflow:hidden;}
	.insight .textarea { height:77px; overflow:hidden;}

	.benefits_inner li button { line-height:normal;}
}

@media all and ( max-width:768px ){

	#content { padding:20px 0; box-sizing:border-box;}
	h3 { font-size:34px;}
	.works { box-sizing:border-box; padding-right:20px; padding-left:20px;}
	.works_inner li { width:100%; }
	.works_inner .play img { width:50%; }
	.works_inner .work_text { top:60%; }
	.works_inner .second { margin:0; margin:10px 0;}
	.visual { box-sizing:border-box; padding-right:20px; padding-left:20px;}
	.developed { box-sizing:border-box; padding-right:20px; padding-left:20px;}
	.developed li { width:100%;}
	.benefits {box-sizing:border-box; padding:20px;}
	.benefits .item1_img li { width:50%;}
	.developed_inner p { padding-bottom:38px;}
	.benefits .text2 h4 { text-align:left; }
	.benefits .text2 { margin-right:0; width:47%; padding-left:0;}
	.benefits .item1_text { width:47%; margin-left:0;  }
	.benefits_inner .center { margin:0;}
	.benefits .item1_img { display:none; }
	.m_benefits { display:block; padding-top:60px;}
	.m_benefits img { width:100%; }
	.features { padding:0 20px; box-sizing:border-box;}
	.features_inner li img { width:50%;}
	.features .top_text { width:100%; }
	
	.features .more { position:static; margin-top:20px;}
	.features_inner li { width:100%; padding-bottom:75px;}
	
	.what { padding-top:70px;}
	.what .what_item1 { width:100%; padding-top:0; padding-left:20px;}
	.what .what_item1 p { padding-bottom:25px;}
	.what .what_btn { float:left; margin-right:10px;}
	.what .what_item2 { width:100%; text-align:center; padding-top:45px;}
	.what .what_item2 img { width:70%; }
	.works_inner .play { width:13%; height:21%; margin-left:-6.5%; top:37%;}
	.works_inner .play img { width:100%; }
	.scroll { box-sizing:border-box; margin:0 20px; }
	.scroll_inner .top_title { font-size:24px; top:55px;}
	.scroll_inner .scroll_btn { right:10px;}
	.contact { width:100%; }
	.product { box-sizing:border-box; padding-right:20px; padding-left:20px;}
	.product_inner li {width:32%; margin:0;}
	.product_inner .center { margin:0 1%;}

	.insight { box-sizing:border-box; padding-right:20px; padding-left:20px; }
	.insight_box li { width:32%; margin:0;}
	.insight_box .center { margin:0 1%;}
	.pc_visual { display:none; }
	.m_visual { display:block;}
}
@media all and ( max-width:650px ){
	.scroll_inner .scroll_btn { top:250px;}
}
@media all and ( max-width:500px ){
	.benefits_inner li button { font-size:14px;}
}

@media all and ( max-width:480px ){
	h3 { font-size:24px; }
	.developed h3 { font-size:24px; }
	.developed_inner strong { font-size:18px; }
	.developed_inner p { font-size:14px;}
	.benefits .item1_text { width:100%; }
	
	
	.benefits .item1_text h4 { font-size:24px;}
	.benefits .item1_text strong { font-size:14px;}
	.benefits .item1_text p { font-size:14px; }
	.benefits .item1_text strong { background-position:0 7px;}
	.features_inner .inner_text { overflow:hidden;}
	.features .top_text { font-size:18px;}
	.features_inner li strong { font-size:18px;}
	.benefits .text2  p { padding-left:50px;}
	.benefits .benefits_inner button { font-size:14px;}
	.benefits h3 { font-size:24px;}
	.what .what_item1 p{ font-size:14px;}
	.product_inner li { width:31%; }
	.scroll_inner .top_title { font-size:24px; top:15px;}
	.scroll_inner .teacher span { font-size:18px;}
	.scroll_inner .us strong { font-size:24px; }
	.scroll_inner .us p { font-size:18px; }
	.benefits_inner .choice:after { left:43%;}
	.features .more { width:250px; height:45px; background-position:190px 5px; }
	.scroll_inner .us .email { font-size:18px; width:250px; }
	.what .what_btn { width:250px; height:45px; background-position:190px 5px; font-size:14px;}
	.insight .pdf { font-size:12px;}
	.product_inner li p { font-size:14px; }
	.insight .scale { font-size:18px;}
	.insight .textarea { font-size:14px;}
	.insight_box span { font-size:14px;} 
	.insight_box li { padding-left:10px;}
	.insight .scale { height:40px;}
	.insight .textarea { height:68px;}
	.product_inner li p { padding-left:10px;}
	.insight_box li { height:200px;}
	
}

