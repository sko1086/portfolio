#visualWrap2 { background:url("../images/visual_sub.jpg") no-repeat; background-size:cover;}
.visual2 { width:980px; margin:0 auto; }
.visual2:after { content:""; display:block; clear:both;}
.visual2 p { padding-top:30px;float:left; padding-bottom:30px; font-size:18px; color:#333; font-weight:100;}
.visual2 ul { float:right; padding-top:40px; padding-bottom:30px;}
.visual2 ul li { float:left; }
.visual2 ul li a { color:#999; }
