.content .table_box h2 { font-size:24px; padding:30px 0;}
.content  h2 span { font-weight:100; font-size:18px;}
.content .table1 { position:relative; }
.content .table_box select { width:140px; height:28px; margin-right:10px; }
.content .table_box button {  position:absolute;  width:40px; height:28px; background:#7c808d url("../images/scale.png") no-repeat 11px; border-radius:5px; font-size:0;}
.content .table_box { padding-top:10px; }
.content .table_box thead { border-bottom:2px solid #ddd;}
.content .table_box th{ border:1px solid #ddd; font-size:13px; text-align:center; padding:5px 5px; background-color:#eef5f8; color:#666; }
.content .table_box table { border:1px solid #ddd; width:980px; border-top:2px solid #6699cc; }
.content .table_box td { border:1px solid #ddd;text-align:center; font-size:12px; padding:12px 12px; color:#666;}
.content .month { padding-bottom:10px;}
.content .table_box td.view a{ color:#336699; text-decoration:underline; font-size:13px; }
.content .month2 { padding-top:50px;}
.content .month2 input { height:22px; margin-right:10px;}
.table_box  { padding-top:10px;}
.table_box .box2_head { border:1px solid #b9c1dc; padding:21px 10px;}
.table_box .box2_head:after { content:""; display:block; clear:both;}
.table_box .box2_head li{ float:left; padding-right:65px;  }
.table_box .box2_head li a{ font-size:13px; color:#333;}
.table_box .box2_head li:first-child {  padding-right:50px; }
.table_box .box2_head li:first-child a{ color:#6699cc; }
.table_box .box2 { padding-top:10px;}
.table_box .box2 tr.bot, .table_box .box2 tr.bot th { background-color:#f6f6f6; color:#333;}
.table_box .box2 tr.bot td { color:#333;}
.table_box table .title th { padding:11px 5px;}
.table_box .box2 td.agree { padding-left:30px; padding-right:30px;}

.list { text-align:center; padding-top:20px; padding-bottom:50px;}
.list_box:after { content:""; display:block; clear:both; }
.list_box { width:260px; margin:0 auto; }
.list ul li a{ float:left; width:20px; height:20px; border:1px solid #eeeeee; margin-left:6px; text-align:center; line-height:20px;}
.list .left1 a{ background:url("../images/arrow_l.png") no-repeat 5px; }
.list .left2 a { background:url("../images/1_arrow_l.png") no-repeat 7px; }
.list .right1 a { background:url("../images/1arrow_r.png") no-repeat 7px; }
.list .right2 a { background:url("../images/arrow_r.png") no-repeat 7px; }
.list .num1 a{ background-color:#c5e0fa; border:1px solid #8cb4db; }


